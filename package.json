{
  "name": "react-router",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn run clean:build && cross-env NODE_ENV=production webpack --config webpack.config.js",
    "clean:build": "cleandir build",
    "eject": "rimraf .git",
    "formatter": "prettier --print-width 60 --no-semi --single-quote --jsx-bracket-same-line  --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\" --fix | snazzy",
    "postinstall": "node ./scripts/semantic.fix.js",
    "generate": "plop --plopfile ./scripts/generators/index.js",
    "precommit": "yarn run lint && lint-staged",
    "server": "node server",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --progress --hot",
    "start:prod": "yarn run build && yarn run server",
    "webpack:statistic": "webpack --json >  stats.json"
  },
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "src/**/*.js": [
      "formatter",
      "lint",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react"
    ]
  },
  "dependencies": {
    "axios": "0.16.1",
    "express": "4.15.3",
    "express-history-api-fallback": "2.2.1",
    "express-static-gzip": "0.2.2",
    "history": "4.6.1",
    "immutable": "3.8.1",
    "preact": "8.1.0",
    "preact-compat": "3.16.0",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-router-redux": "next",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "semantic-ui-react": "0.68.3",
    "styled-components": "1.4.6"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "bundle-loader": "0.5.5",
    "clean-dir": "1.0.3",
    "compression-webpack-plugin": "0.4.0",
    "cross-env": "5.0.0",
    "css-loader": "0.28.1",
    "env-cmd": "5.1.0",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.3",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "less-plugin-autoprefix": "1.5.1",
    "less-plugin-clean-css": "1.5.1",
    "lint-staged": "3.4.2",
    "offline-plugin": "4.8.1",
    "plop": "1.7.4",
    "postcss-loader": "2.0.5",
    "prettier": "1.3.1",
    "redux-devtools": "3.4.0",
    "redux-logger": "3.0.6",
    "rimraf": "2.6.1",
    "semantic-ui-less": "2.2.10",
    "snazzy": "7.0.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.17.0",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "react/prop-types": 0
    }
  },
  "_moduleAliases": {
    "@root": ".",
    "@utils": "utils"
  }
}
